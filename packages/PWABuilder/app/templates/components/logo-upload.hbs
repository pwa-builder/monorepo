{{input value=logoUrl type="url" placeholder="http://example.com/image.png or /images/example.png"}}
<div class="button-holder">
	<button class="add-button" {{action 'addLogo'}}>Add Icon&nbsp;&nbsp; {{fa-icon "plus"}}</button>
</div>
<div class="logos">
	{{#if logos}}
		<table class="generator-members">
			<thead>
				<tr>
					<th>Preview</th>
					<th>Size</th>
					<th></th>
				</tr>
			</thead>
			<tbody>
	  	{{#each logo in logos}}
		    <tr>
          <td><a href="{{logo.imgSrc}}" target="_blank">{{icon-display src=logo.src base=baseUrl}}</a></td>
		      <td>{{logo.sizes}}</td>
		      <td class="button-holder"><button {{action 'removeLogo' logo}}>{{fa-icon "times"}}</button></td>
		    </tr>
	  	{{/each}}
	  		</tbody>
	  	</table>
	{{/if}}
</div>

